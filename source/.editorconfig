# This file contains global code style settings that affect all files/projects in the repository.
# Individual directories might contain more specific .editorconfig files that override these settings.
# Documentation:
# - https://editorconfig.org/
# - https://docs.microsoft.com/en-us/visualstudio/ide/create-portable-custom-editor-options
# - https://www.jetbrains.com/help/resharper/EditorConfig_Index.html

root = true

[*]
charset = utf-8
indent_style = space
# Do not specify indent_size here.
end_of_line = crlf
insert_final_newline = true
trim_trailing_whitespace = true

# Attention: if you modify the editorconfig file with the ReSharper editor, it will often place the settings here.
# You may want to instead move these settings into the appropriate sections further down in the file (or place
# them into more specific editorconfig files), depending on what you intended with the changes.

# There should be no new line at the end of CSV files, because it would be interpreted as an empty entry
[*.csv]
insert_final_newline = false

[*.cs]
charset = utf-8-bom
indent_style = space
indent_size = 4
max_line_length = 160
tab_width = 4

[*.{csproj,props,targets,xml,resx,config}]
indent_size = 2
# max_line_length = off restores the editors default behavior (e.g. Rider uses 120 chars)
# see https://github.com/editorconfig/editorconfig/issues/321
# We want "unlimited" line length, therefore we set a very large number
max_line_length = 999999
resharper_xml_space_before_self_closing = true
resharper_max_blank_lines_between_tags = 1
resharper_xml_blank_line_after_pi = false

[*.{yml,yaml}]
indent_size = 2

[*.ps1]
indent_size = 4

[*.md]
# see info above
max_line_length = 999999
trim_trailing_whitespace = false
# Trailing whitespace can be significant in Markdown.

### C# Code Style

[*.cs]
# Formatting issues shall be reported as warnings
dotnet_diagnostic.IDE0055.severity = warning

# No file header.
file_header_template = unset

# Usings
dotnet_sort_system_directives_first = true # `using System.*;` comes first.
dotnet_separate_import_directive_groups = true # we want separated using imports as those are easier to read
csharp_using_directive_placement = outside_namespace:warning

# Prefer file-scoped namespaces, and match the folder name.
csharp_style_namespace_declarations = file_scoped:warning    # IDE0160: Use block-scoped namespace / IDE0161: Use file-scoped namespace
dotnet_style_namespace_match_folder = true:warning           # IDE0130: Namespace does not match folder structure
csharp_style_prefer_method_group_conversion = true:warning   # IDE0200: Remove unnecessary lambda expression
csharp_style_prefer_primary_constructors = true:warning      # IDE0290: Use primary constructor

csharp_style_prefer_top_level_statements = true:warning      # IDE0210: Convert to top-level statements

# Avoid unnecessary `this.` qualification.
dotnet_diagnostic.IDE0003.severity = warning
dotnet_diagnostic.IDE0009.severity = none
dotnet_style_qualification_for_field = false:warning
dotnet_style_qualification_for_property = false:warning
dotnet_style_qualification_for_method = false:warning
dotnet_style_qualification_for_event = false:warning

# Use language keywords instead of framework types
dotnet_style_predefined_type_for_locals_parameters_members = true:warning
dotnet_style_predefined_type_for_member_access = true:warning

# Modifier preferences
dotnet_style_require_accessibility_modifiers = for_non_interface_members:warning   # IDE0040 : Add accessibility modifiers
csharp_preferred_modifier_order = public,private,protected,internal,file,static,extern,new,virtual,abstract,sealed,override,readonly,unsafe,required,volatile,async

dotnet_style_readonly_field = true:warning                         # IDE0044: Add readonly modifier
csharp_style_prefer_readonly_struct = true:warning                 # IDE0250: Struct can be made 'readonly'
csharp_style_prefer_readonly_struct_member = true:warning          # IDE0251: Member can be made 'readonly'
csharp_prefer_static_local_function = true:warning                 # IDE0062: Make local function static
csharp_style_prefer_local_over_anonymous_function = true:warning   # IDE0039: Use local function instead of lambda

# Expression-level preferences
dotnet_style_operator_placement_when_wrapping  = beginning_of_line  # Operator placement
csharp_style_prefer_index_operator = true:warning                   # IDE0056: Use index operator
csharp_style_prefer_range_operator = true:warning                   # IDE0057: Use range operator

# Initializers
dotnet_style_object_initializer = true:warning             # IDE0017: Use object initializers
dotnet_style_collection_initializer = true:warning         # IDE0028: Use collection initializers or expressions
dotnet_style_prefer_collection_expression = true:warning   # IDE0300: Use collection expression for array

# Null handling
# IDE0029: Null check can be simplified (ternary conditional check)
# IDE0030: Null check can be simplified (nullable ternary conditional check)
# IDE0270: Null check can be simplified (if null check) ... xyz is null comparisons
dotnet_diagnostic.IDE0270.severity = none         
dotnet_style_coalesce_expression = true:warning                                   # IDE0029 and IDE0030 are warnings
dotnet_style_null_propagation = true:warning                                      # IDE0031: Use null propagation
dotnet_style_prefer_is_null_check_over_reference_equality_method = true:warning   # IDE0041: Use 'is null' check
csharp_style_prefer_null_check_over_type_check = true:warning                     # IDE0150: Prefer 'null' check over type check

# Auto properties
dotnet_style_prefer_auto_properties = true:warning   # IDE0032: Use auto-implemented property

# Conditional expressions
csharp_style_conditional_delegate_call = true:warning                      # IDE1005: Use conditional delegate call
csharp_prefer_simple_default_expression = true:warning                     # IDE0034: Simplify 'default' expression
dotnet_style_prefer_conditional_expression_over_assignment = true:silent   # IDE0045: Use conditional expression for assignment
dotnet_style_prefer_conditional_expression_over_return = true:silent       # IDE0046: Use conditional expression for return

# Pattern matching
# IDE0020: Use pattern matching to avoid is check followed by a cast (with variable)
# IDE0038: Use pattern matching to avoid is check followed by a cast (without variable)
csharp_style_pattern_matching_over_is_with_cast_check = true:warning

csharp_style_pattern_matching_over_as_with_null_check = true:warning  # IDE0260: Use pattern matching / IDE0019: Use pattern matching to avoid 'as' followed by a 'null' check
csharp_style_prefer_pattern_matching = true:warning                   # IDE0078: Use pattern matching
csharp_style_prefer_not_pattern = true:warning                        # IDE0083: Use pattern matching (not operator)


# Tuples
dotnet_style_explicit_tuple_names = true:warning                          # IDE0033: Use explicitly provided tuple name
csharp_style_prefer_tuple_swap = true:warning                             # IDE0180: Prefer using a tuple to swap two values
dotnet_style_prefer_inferred_tuple_names = true:warning                   # IDE0037: Use inferred member names
dotnet_style_prefer_inferred_anonymous_type_member_names = true:warning   # IDE0037: Use inferred member names


# Discards
csharp_style_unused_value_expression_statement_preference = discard_variable:warning   # IDE0058: Remove unnecessary expression value
csharp_style_unused_value_assignment_preference = discard_variable:warning             # IDE0059: Remove unnecessary value assignment

# Other style preferences
csharp_prefer_simple_using_statement = true:warning                 # IDE0063: Use simple 'using' statement
csharp_style_prefer_switch_expression = true:suggestion             # IDE0066: Use switch expression
csharp_style_inlined_variable_declaration = true:warning            # IDE0018: Inline variable declaration
csharp_style_deconstructed_variable_declaration = true:warning      # IDE0042: Deconstruct variable declaration
csharp_style_throw_expression = true:suggestion                     # IDE0016: Use throw expression
dotnet_style_prefer_simplified_interpolation = true:warning         # IDE0071: Simplify interpolation
dotnet_style_prefer_simplified_boolean_expressions = true:warning   # IDE0075: Simplify conditional expression
dotnet_code_quality_unused_parameters = all:warning                 # IDE0060: Remove unused parameter

# Prefer curly braces wherever possible and sensible
csharp_prefer_braces = true:warning   # IDE0011: Add braces

csharp_preserve_single_line_blocks = true
csharp_preserve_single_line_statements = true

csharp_new_line_before_open_brace = all     # Allman style
csharp_new_line_before_else = true
csharp_new_line_before_catch = true
csharp_new_line_before_finally = true
csharp_new_line_before_members_in_object_initializers = true
csharp_new_line_before_members_in_anonymous_types = true
csharp_new_line_between_query_expression_clauses = true

# ReSharper properties
csharp_brace_style = next_line
csharp_braces_for_ifelse = required
csharp_braces_for_for = required
csharp_braces_for_foreach = required
csharp_braces_for_while = required
csharp_braces_for_dowhile = required
csharp_braces_for_using = required
csharp_braces_for_lock = required
csharp_braces_for_fixed = required

# Indentation
csharp_indent_block_contents = true
csharp_indent_braces = false
csharp_indent_case_contents = true
csharp_indent_case_contents_when_block = true
csharp_indent_switch_labels = true
csharp_indent_labels = flush_left

# Alignment
# ReSharper properties
resharper_align_first_arg_by_paren = true
resharper_align_linq_query = true
resharper_align_multiline_argument = true
resharper_align_multiline_calls_chain = true
resharper_align_multiline_expression = true
resharper_align_multiline_extends_list = true
resharper_align_multiline_parameter = true
resharper_align_multiple_declaration = true
resharper_align_multline_type_parameter_constrains = true
resharper_align_multline_type_parameter_list = true
resharper_align_tuple_components = true

# Prefer `var` over explicit types
# IDE0007: Use 'var' instead of explicit type / IDE0008: Use explicit type instead of 'var'
csharp_style_var_for_built_in_types = true:warning
csharp_style_var_when_type_is_apparent = true:warning
csharp_style_var_elsewhere = true:warning

# Prefer explicitly typed object creation
# Target typed new
csharp_style_implicit_object_creation_when_type_is_apparent = true:warning   # IDE0090: Simplify 'new' expression

# ReSharper properties
csharp_object_creation_when_type_evident = target_typed    # Omit type: 'new()'

# Prefer method-like constructs to have an expression-body
csharp_style_expression_bodied_constructors = when_on_single_line:warning   # IDE0021: Use expression body for constructors
csharp_style_expression_bodied_methods = when_on_single_line:warning        # IDE0022: Use expression body for methods
csharp_style_expression_bodied_operators = when_on_single_line:warning      # IDE0023: Use expression body for conversion operators / # IDE0024: Use expression body for operators
csharp_style_expression_bodied_local_functions = false:none                 # (do not enforce) IDE0061: Use expression body for local functions

# Prefer property-like constructs to have an expression-body (but don't enforce it)
csharp_style_expression_bodied_properties = when_on_single_line:warning   # IDE0025: Use expression body for properties
csharp_style_expression_bodied_indexers = when_on_single_line:warning     # IDE0026: Use expression body for indexers
csharp_style_expression_bodied_accessors = when_on_single_line:warning    # IDE0027: Use expression body for accessors
csharp_style_expression_bodied_lambdas = when_on_single_line:warning      # IDE0053: Use expression body for lambdas

# Prefer parentheses for clarity, but don't enforce them.
dotnet_style_parentheses_in_arithmetic_binary_operators = always_for_clarity:silent
dotnet_style_parentheses_in_relational_binary_operators = always_for_clarity:silent
dotnet_style_parentheses_in_other_binary_operators = always_for_clarity:silent
dotnet_style_parentheses_in_other_operators = never_if_unnecessary:silent

# IDE0054: Use compound assignment
# IDE0074: Use coalesce compound assignment
dotnet_style_prefer_compound_assignment = true:warning

csharp_style_prefer_extended_property_pattern = true:warning # IDE0170: Simplify property pattern

# Whitespace rules
csharp_preserve_single_line_blocks = true
csharp_space_after_cast = false
csharp_space_after_colon_in_inheritance_clause = true
csharp_space_before_colon_in_inheritance_clause = true
csharp_space_after_comma = true
csharp_space_before_comma = false
csharp_space_after_dot = false
csharp_space_before_dot = false
csharp_space_after_keywords_in_control_flow_statements = true
csharp_space_after_semicolon_in_for_statement = true
csharp_space_before_semicolon_in_for_statement = false
csharp_space_around_binary_operators = before_and_after
csharp_space_around_declaration_statements = do_not_ignore
csharp_space_before_open_square_brackets = false
csharp_space_between_empty_square_brackets = false
csharp_space_between_square_brackets = false
csharp_space_between_method_call_empty_parameter_list_parentheses = false
csharp_space_between_method_call_name_and_opening_parenthesis = false
csharp_space_between_method_call_parameter_list_parentheses = false
csharp_space_between_method_declaration_empty_parameter_list_parentheses = false
csharp_space_between_method_declaration_name_and_open_parenthesis = false
csharp_space_between_method_declaration_parameter_list_parentheses = false
csharp_space_between_parentheses = false
# ReSharper properties
resharper_space_within_single_line_array_initializer_braces = true
resharper_csharp_keep_blank_lines_in_declarations = 1
# ReSharper inspections
resharper_missing_blank_lines_highlighting = warning
resharper_redundant_blank_lines_highlighting = warning
resharper_incorrect_blank_lines_near_braces_highlighting = warning

# White space rules (experimental)
dotnet_style_allow_multiple_blank_lines_experimental = false:warning                     # IDE2000: Allow multiple blank lines
csharp_style_allow_embedded_statements_on_same_line_experimental = false:warning         # IDE2001: Allow embedded statements on same line
csharp_style_allow_blank_lines_between_consecutive_braces_experimental = false:warning   # IDE2002: Allow blank lines between consecutive braces

## TODO: csharp_style_allow_statement_immediately_after_block_experimental = false:warning        # IDE2003: Allow statement immediately after block

csharp_style_allow_blank_line_after_colon_in_constructor_initializer_experimental = false:warning  # IDE2004: Allow blank line after colon in constructor initializer
csharp_style_allow_blank_line_after_token_in_conditional_expression_experimental = false:warning   # IDE2005: Allow blank line after token in conditional expression
csharp_style_allow_blank_line_after_token_in_arrow_expression_clause_experimental = false:warning  # IDE2006: Allow blank line after token in arrow expression clause
csharp_style_allow_blank_line_after_colon_in_object_initializer_experimental = false:warning       # IDE2007: Allow blank line after token in object initializer

# String literals
csharp_style_prefer_utf8_string_literals = true:warning # IDE0230: Use UTF-8 string literal

# Wrapping and line breaks
csharp_new_line_before_members_in_object_initializers = true
# ReSharper properties
resharper_csharp_wrap_multiple_declaration_style = chop_always
resharper_keep_existing_switch_expression_arrangement = false
resharper_keep_existing_initializer_arrangement = true
resharper_max_enum_members_on_line = 0
# ReSharper inspections
resharper_missing_linebreak_highlighting = warning
resharper_redundant_linebreak_highlighting = warning
resharper_multiple_statements_on_one_line_highlighting = warning

# Attributes are separated
# ReSharper properties
resharper_force_attribute_style = separate
resharper_place_accessorholder_attribute_on_same_line = false
resharper_place_field_attribute_on_same_line = false
# ReSharper inspection severities
resharper_arrange_attributes_highlighting = hint

# XML documentation comments
# ReSharper properties
xmldoc_indent_child_elements = zero_indent
xmldoc_indent_text = zero_indent

### Symbol naming rules

# Constants are PascalCase
dotnet_naming_rule.constants_should_be_pascal_case.severity = warning
dotnet_naming_rule.constants_should_be_pascal_case.symbols = constants
dotnet_naming_rule.constants_should_be_pascal_case.style = constant_style
dotnet_naming_symbols.constants.applicable_kinds = field, local
dotnet_naming_symbols.constants.required_modifiers = const
dotnet_naming_style.constant_style.capitalization = pascal_case

# Static readonly fields are PascalCase
dotnet_naming_rule.static_fields_should_be_camel_case.severity = warning
dotnet_naming_rule.static_fields_should_be_camel_case.symbols = static_fields
dotnet_naming_rule.static_fields_should_be_camel_case.style = static_field_style
dotnet_naming_symbols.static_fields.applicable_kinds = field
dotnet_naming_symbols.static_fields.required_modifiers = static, readonly
dotnet_naming_style.static_field_style.capitalization = pascal_case

# Instance fields are camelCase and start with _
dotnet_naming_rule.instance_fields_should_be_camel_case.severity = warning
dotnet_naming_rule.instance_fields_should_be_camel_case.symbols = instance_fields
dotnet_naming_rule.instance_fields_should_be_camel_case.style = instance_field_style
dotnet_naming_symbols.instance_fields.applicable_kinds = field
dotnet_naming_style.instance_field_style.capitalization = camel_case
dotnet_naming_style.instance_field_style.required_prefix = _

# Async methods should have an Async suffix
dotnet_naming_rule.async_methods_should_have_suffix.severity = warning
dotnet_naming_rule.async_methods_should_have_suffix.symbols = async_methods
dotnet_naming_rule.async_methods_should_have_suffix.style = async_method_style
dotnet_naming_symbols.async_methods.applicable_kinds = method
dotnet_naming_symbols.async_methods.required_modifiers = async
dotnet_naming_style.async_method_style.capitalization = pascal_case
dotnet_naming_style.async_method_style.required_suffix = Async


